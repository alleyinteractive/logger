{
    "name": "alleyinteractive/logger",
    "description": "A logger tool that stores errors and messages as a custom post type.",
    "homepage": "https://github.com/alleyinteractive/logger",
    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "config": {
      "optimize-autoloader": true,
      "apcu-autoloader": true
    },
    "authors": [
        {
            "name": "Alley Interactive",
            "email": "info@alley.co"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/alleyinteractive/Alley-Coding-Standards"
        }
    ],
    "require-dev": {
        "alley/alley-coding-standards": "dev-master"
    },
    "scripts": {
        "phpcs" : "phpcs --standard=./phpcs.xml ."
    },
    "autoload": {
        "classmap": ["inc/"]
    },
    "require": {
        "psr/log": "^1.1"
    }
}
